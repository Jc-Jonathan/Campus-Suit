{
  "expo": {
    "name": "Exotic Store",
    "slug": "Campus-Suit",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "campussuit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/scholarship.jpg",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0080FF",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/scholarship.jpg",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/scholarship.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/scholarship.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0080FF",
          "dark": {
            "backgroundColor": "#0080FF"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
